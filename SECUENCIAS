CREATE TABLE producto
(
    id_producto NUMBER (5),
    descripcion VARCHAR2(50)
);

INSERT INTO producto VALUES (5467, 'silla de descanso');
INSERT INTO producto VALUES (5469, 'mesa de centro');

CREATE SEQUENCE SEQ_PRODUCTO_ID
INCREMENT BY 1
START WITH 1
MAXVALUE 20
CYCLE
--NOCYCLE
NOCACHE;

DROP SEQUENCE SEQ_PRODUCTO_ID;

SELECT SEQ_PRODUCTO_ID.NEXTVAL
FROM DUAL;

SELECT SQ_PRODUCTO_ID.CURRVAL
FROM DUAL;

-- UTILIZAR SECUENCIA PARA INSERTAR DATOS
INSERT INTO producto
VALUES (SEQ_PRODUCTO_ID.NEXTVAL, 'mouse inalambrico'); 

INSERT INTO producto
VALUES (SEQ_PRODUCTO_ID.NEXTVAL, 'mousepad');
